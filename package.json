{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@apollo/client": "^3.6.10",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-loader-spinner": "^5.3.4",
    "react-router-dom": "^6.4.1"
  },
  "scripts": {
    "build": "webpack",
    "generate-types": "graphql-codegen && eslint app/ui/graphql/types --fix --quiet",
