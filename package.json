    "format": "prettier --config .prettierrc 'app/ui/**/*.ts' 'app/ui/**/*.tsx' 'app/javascript/**/*.ts' 'app/javascript/**/*.tsx' --write --no-error-on-unmatched-pattern"
  },
  "devDependencies": {
