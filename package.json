    "jest-transform-graphql": "^2.1.0",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.3",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
