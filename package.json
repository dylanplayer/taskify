{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@apollo/client": "^3.6.10",
    "esbuild": "^0.15.9",
    "esbuild-css-modules-plugin": "^2.5.2",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.4.1"
  },
  "scripts": {
    "build": "chmod +x build.js && ./build.js",
    "build1": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "ts-check": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint app/javascript app/ui --fix",
    "test": "jest --passWithNoTests",
    "format": "prettier --config .prettierrc 'app/ui/**/*.ts' 'app/ui/**/*.tsx' 'app/javascript/**/*.ts' 'app/javascript/**/*.tsx' --write --no-error-on-unmatched-pattern"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.10.0",
    "eslint-plugin-react": "^7.31.8",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3",
    "typescript-plugin-css-modules": "^3.4.0"
  }
}
